{
  "manifest_version": 3,
  "name": "Memento",
  "version": "1.0",
  "description": "A Chrome extension with full onboarding page",
  "permissions": ["sidePanel","storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Memento"
  },
  "web_accessible_resources": [
    {
      "resources": ["onboarding.html", "onboarding.js", "static/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "side_panel": {
    "default_path": "index.html"
  }
}

