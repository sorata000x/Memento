{
  "manifest_version": 3,
  "name": "Memento",
  "version": "1.3.1",
  "description": "",
  "permissions": ["sidePanel", "alarm", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    },
    "default_title": "Memento"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html", "onboarding.html", "onboarding.js", "static/*", "setting.html", "setting.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "side_panel": {
    "default_path": "index.html"
  }
}

